{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_id": {
      "type": "uid",
      "required": true
    },
    "product_name": {
      "type": "string",
      "required": true
    },
    "descriptions": {
      "type": "text",
      "required": true
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "white",
        "black",
        "red",
        "green",
        "blue",
        "yellow"
      ],
      "required": true
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "S",
        "M",
        "L",
        "XL",
        "XXL",
        "XXXL"
      ],
      "required": true
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "products"
    },
    "price": {
      "type": "float",
      "required": true
    },
    "stocklevel": {
      "type": "integer",
      "required": true
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "products"
    },
    "product_image": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    }
  }
}
