{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "product_name": {
      "type": "string",
      "required": true
    },
    "descriptions": {
      "type": "text",
      "required": true
    },
    "price": {
      "type": "float",
      "required": true
    },
    "reviews": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::review.review",
      "mappedBy": "products"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "products"
    },
    "product_images": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::product-image.product-image",
      "mappedBy": "product"
    },
    "inventories": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::inventory.inventory",
      "mappedBy": "product"
    },
    "shopping_cart": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::shopping-cart.shopping-cart",
      "inversedBy": "products"
    }
  }
}
