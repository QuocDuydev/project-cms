{
  "kind": "collectionType",
  "collectionName": "product_images",
  "info": {
    "singularName": "product-image",
    "pluralName": "product-images",
    "displayName": "Product_image"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "image_name": {
      "type": "string",
      "required": true
    },
    "image_url": {
      "allowedTypes": [
        "images"
      ],
      "type": "media",
      "multiple": true,
      "required": true
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "white",
        "black",
        "red",
        "green",
        "blue",
        "yellow"
      ],
      "required": true
    },
    "size": {
      "type": "enumeration",
      "enum": [
        "S",
        "M",
        "L",
        "XL",
        "XXL",
        "XXXL"
      ],
      "required": true
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "product_images"
    }
  }
}
