{
  "kind": "collectionType",
  "collectionName": "reviews",
  "info": {
    "singularName": "review",
    "pluralName": "reviews",
    "displayName": "Review",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "rating": {
      "type": "integer",
      "required": true
    },
    "review_text": {
      "type": "text",
      "required": true
    },
    "review_date": {
      "type": "datetime",
      "required": true
    },
    "status": {
      "type": "enumeration",
      "enum": [
        "unconfimred",
        "confirm"
      ],
      "required": true,
      "default": "unconfimred"
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "reviews"
    },
    "review_images": {
      "type": "media",
      "multiple": true,
      "required": true,
      "allowedTypes": [
        "images",
        "videos",
        "audios",
        "files"
      ]
    }
  }
}
